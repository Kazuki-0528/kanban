<div class="topPage">
  <div class="listWrapper">
    <% @list.each do |list| %>
      <div class="list">
        <div class="list_header">
          <h2 class="list_header_title"><%= list.title %></h2>
          <div class="list_header_action">
            <%= link_to '削除', list, method: :delete, data: { confirm: "#{list.title}を削除してもいい？" }, class: 'btn btn-danger' %>
            <%= link_to '編集', edit_list_path(list), class: 'btn btn-info' %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>